{
  "name": "hono-debug",
  "version": "1.1.1",
  "description": "A Hono middleware making it easy to debug.",
  "author": "nswbmw",
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "scripts": {
    "lint": "eslint .",
    "build": "tsup index.js --format esm,cjs",
    "prepublishOnly": "npm run lint && npm run build"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "CHANGELOG.md",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nswbmw/hono-debug.git"
  },
  "keywords": [
    "hono",
    "middleware",
    "debug",
    "debugging",
    "debugger",
    "troubleshoot",
    "profile",
    "log",
    "logging",
    "breakpoint",
    "error"
  ],
  "devDependencies": {
    "@hono/node-server": "1.19.1",
    "eslint": "8.57.0",
    "eslint-config-standard": "17.1.0",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-n": "17.21.3",
    "eslint-plugin-promise": "6.1.1",
    "hono": "4.9.6",
    "tsup": "8.5.0"
  }
}
